{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request",
        "maxTelemetryItemsPerSecond": 20
      }
    },
    "logLevel": {
      "default": "Warning",
      "Function": "Information",
      "Function.function": "Information",
      "Host.Function.Console": "Information",
      "Vault": "Debug"
    }
  },
  "extensions": {
    "http": {
      "routePrefix": ""
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  },
  "customHandler": {
    "enableForwardingHttpRequest": true,
    "description": {
      "defaultExecutablePath": "vault-launcher",
      "arguments": [
        "./vault",
        "server",
        "-config",
        "./config/vault.hcl.tpl"
      ]
    }
  }
}
